{
    "name": "vitorleonel/laravel-unique-slug",
    "description": "Package for generate a unique slug in your application on an eloquent model with have slug column.",
    "keywords": ["laravel", "slug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vitor Leonel",
            "email": "falecom@vitorleonel.com"
        }
    ],
    "require": {
        "php" : ">=7.1.3",
        "illuminate/database": "^5.5 || 5.6.* || 5.7.* || 5.8.* || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "VitorLeonel\\UniqueSlug\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VitorLeonel\\UniqueSlug\\UniqueSlugServiceProvider"
            ],
            "aliases": {
                "UniqueSlug": "VitorLeonel\\UniqueSlug\\UniqueSlugFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
